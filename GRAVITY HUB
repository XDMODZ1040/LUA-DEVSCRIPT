local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()

local Window = Library.CreateLib("GRAVITY HUB | V1", "Ocean", "Lua")

local PlrTab = Window:NewTab("HOME")
local PlrSection = PlrTab:NewSection("Welcome, " .. game.Players.LocalPlayer.DisplayName)
local PlrSection = PlrTab:NewSection("ALL PLANET HAD THERE OWN GRAVITATIONAL")

local PlrTab = Window:NewTab("MENU")
local PlrSection = PlrTab:NewSection("PLANET GRAVITY")

PlrSection:NewButton("MOON", "MOON GRAVITY", function()
Workspace.Gravity = 50
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 5
print("MOON GRAVITY ACTIVATE")
end)

